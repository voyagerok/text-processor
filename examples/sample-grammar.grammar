%rules
PassportSeries = num(len: 4);
PassportNumber = num(len: 6);

Address = Town Street;

%commands
Find PassportSeries(hint_words: "наймодатель") as OwnerPassportSeries;
Find PassportNumber(hint_words: "наниматель");
Find PersonName(hint_words: "наниматель") as Employer;
Find PassportSeries(hint_words: "наниматель", "серия") as EmployerPassportSeries;
Find FullDate(hint_words: "помещение", "предоставить", "наниматель");
Find Address(hint_words: "наниматель");
